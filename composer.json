{
    "name": "rtheunissen/cache",
    "description": "A basic cache interface with adapters for common cache libraries",
    "keywords": ["cache", "adapter", "interface"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rudi Theunissen",
            "email": "rudolf.theunissen@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "doctrine/cache": "~1.4",
        "illuminate/contracts": "~5.1",
        "tedivm/stash": "~0.12"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "Concat\\Cache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Concat\\Cache\\Test\\": "tests"
        }
    }
}
